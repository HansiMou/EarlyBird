               <li><a target="_blank" href="http://chemport.cas.org/cgi-bin/sdcgi?APP=ftslink&amp;action=reflink&amp;origin=wiley&amp;version=1%2E0&amp;coi=1%3aCAS%3a528%3aDC%252BC3cXos1Cqsg%253D%253D&amp;md5=c92718628cc7603169fe37c47b633117">CAS</a> | </li>
               <li><a target="_blank" href="http://gateway.isiknowledge.com/gateway/Gateway.cgi?GWVersion=2&amp;SrcApp=Wiley_Online_Library&amp;SrcAuth=LinksAMR&amp;KeyUT=000273820700012&amp;DestLinkType=FullRecord&amp;DestApp=ALL_WOS&amp;UsrCustomerID=5d29b08d2a4045cc6e96df01e7663ac0">Web of Science® Times Cited: 114</a> | </li>
               <li><a target="_blank" href="http://adsabs.harvard.edu/abs/2010NatCh...2...96V">ADS</a></li>
              </ul></li>
             <li id="chem201600208-bib-0027d"><span class="bullet">27d</span><cite id="chem201600208-cit-0072"><span class="author">D. A. Ossipov</span>, <span class="author">X. Yang</span>, <span class="author">O. Varghese</span>, <span class="author">S. Kootala</span>, <span class="author">J. Hilborn</span>, <span class="journalTitle">Chem. Commun.</span> <span class="pubYear">2010</span>, <span class="vol">46</span>, <span class="pageFirst">8368</span>–<span class="pageLast">8370</span>;</cite>
