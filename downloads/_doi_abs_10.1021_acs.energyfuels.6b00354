  <meta name="dc.Title" content="Combustion Characteristics of Biomass Charcoals Produced at Different Carbonization Conditions: A Kinetic Study">
  <meta name="dc.Creator" content="Liang Wang">
  <meta name="dc.Creator" content="Gábor Várhegyi">
  <meta name="dc.Creator" content="Øyvind Skreiberg">
