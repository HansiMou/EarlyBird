        var done = false;
        if(ACSPubs.login.listenerActive == false){
            ACSPubs.login.listenerActive= true;
            var interval = setInterval(function(){
                if(document.cookie.match(/(?:^|;) ?ERIGHTS=([^;$]+)(?:;|$)/)){
                    done = true;
                    listenerActive = false;
                    clearInterval(interval);
                    return callback();
                }
            }, 1000);
        }    
